<div class="author">
  <h1 class="author_heading">Search Results</h1>
</div>

<section class="articles">
  <ul class="article-ul">
    <% data.forEach(function(post) { %>
      <li>
        <a href="/post/<%= post._id %>">
          <span>
            <%= post.title %>
          </span>
          <span class="article-list_date">
            <%= post.createAt.toDateString() %>
          </span>
        </a>
      </li>
      <% }); %>
  </ul>
</section>