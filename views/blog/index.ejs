<!-- github section -->
<section class="section" id="service">
  <div class="container text-center">
    <p class="section-subtitle">What I Did ?</p>
    <h6 class="section-title mb-6">GitHub Contributions</h6>
    <!-- row -->
    <div class="row">
      <div class="col-md-12 col-lg-12">
        <div class="">
          <!-- service-card -->
          <div class="heatmap-container">
            <div class="month-labels">
              <% for (let i = 0; i < 53; i++) { %>
                <% const month = monthLabels.find(label=> label.weekIndex === i); %>
                <div class="month-label"><%= month ? month.month : '' %></div>
              <% } %>
            </div>
            <div class="heatmap">
            <% contributionDays.forEach(day => { %>
              <div 
                class="day contribution-<%= 
                  day.contributionCount === 0 ? 'none' : 
                  day.contributionCount < 3 ? 'low' : 
                  day.contributionCount < 5 ? 'medium' : 
                  day.contributionCount < 10 ? 'high' : 'very-high' 
                %>"
                title="<%= day.date %>: <%= day.contributionCount %> contributions"
              ></div>
            <% }) %>
          </div>
        </div>

        <div class="container px-4">
          <!-- <h3 style="color: #24292e; font-size: 16px; margin-bottom: 10px;">Activity Types</h3> -->
          <div class="row gx-5">
            <div class="col">
              <div class="p-3">Total Commits</div>
              <h3 style="color: #24292e; font-size: 24px; margin-bottom: 10px;"><%= totalCommits.toLocaleString() %></h3>
                <!-- % commitStats.byRepo.forEach(repo => { %> -->
                  <!-- <li>%= repo.name %>: %= repo.count %> commits</li> -->
                <!-- % }); %> -->
            </div>
            <div class="col">
              <div class="p-3">Total Contributions</div>
              <h3 style="color: #24292e; font-size: 24px; margin-bottom: 10px;"><%= totalContributions.toLocaleString() %></h3>
            </div>
          </div>
        </div>
      </div>
    </div><!-- end of row -->
  </div>
</section><!-- end of github section -->

