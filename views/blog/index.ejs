<script>
  document.addEventListener('DOMContentLoaded', function () {
  function getColor(count) {
    if (count === 0) return '#ebedf0'; // No contributions
    if (count < 5) return '#9be9a8';  // Low
    if (count < 10) return '#40c463'; // Medium
    if (count < 20) return '#30a14e'; // High
    return '#216e39'; // Very high
  }
});
</script>